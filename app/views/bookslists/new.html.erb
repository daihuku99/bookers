<h1>New book</h1>

<%= form_for(@list, url: '/bookslists') do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>
   <% if @list.errors.any? %>
   		<p>error</p>
   <% end %>
	<h4>Title</h4>
	<%= f.text_field :title %>

	<h4>Body</h4>
	<%= f.text_field :body %>

	<%= f.submit 'Create Book' %>
<% end %>